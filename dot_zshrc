# Lines configured by zsh-newuser-install
HISTFILE=~/.histfile
HISTSIZE=10000
SAVEHIST=50000
bindkey -e
# End of lines configured by zsh-newuser-install
# The following lines were added by compinstall
zstyle :compinstall filename '/home/kenneth/.zshrc'

autoload -Uz compinit
compinit
# End of lines added by compinstall

if [[ -f ~/.profile ]]; then
    source ~/.profile
fi

# Aliases
if [ -f ~/.shell_aliases ]; then
  . ~/.shell_aliases
fi

ytdlpmp3() {
    yt-dlp --extract-audio --audio-format mp3 --audio-quality 0 "$1"
}

bindkey -s ^f "tmux-sessionizer\n"

eval "$(starship init zsh)"
eval "$(zoxide init zsh)"

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/home/kenneth/Downloads/google-cloud-sdk/path.zsh.inc' ]; then . '/home/kenneth/Downloads/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/home/kenneth/Downloads/google-cloud-sdk/completion.zsh.inc' ]; then . '/home/kenneth/Downloads/google-cloud-sdk/completion.zsh.inc'; fi

export PATH=$PATH:/home/kenneth/.spicetify
